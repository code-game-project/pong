name pong
version 0.1

event move {
	direction: enum direction {
		none, up, down
	}
}

event positions {
	ball: rectangle,
	paddle_left: rectangle,
	paddle_right: rectangle
}

event score {
	player_left: int,
	player_right: int
}

type rectangle {
	x: float,
	y: float,
	width: float,
	height: float
}
